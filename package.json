{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "start": "webpack-dev-server --env.development --config ./build/webpack.dev.config.js",
    "build": "webpack --env.production --config ./build/webpack.prod.config.js",
    "dev-build": "webpack --profile --json > stats.json --config ./build/webpack.dev.config.js",
    "analyze": "NODE_ENV=production npm_config_report=true npm run build",
    "lint": "eslint --cache --fix src"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^7.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "redux-devtools": "^3.5.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.4.3",
    "typescript": "^3.4.4",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "antd": "^3.19.4",
    "babel-plugin-import": "^1.12.0",
    "core-js": "2",
    "cross-env": "^5.2.0",
    "eslint-plugin-babel": "^5.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-starter-kit": "^0.5.1",
    "redux-thunk": "^2.3.0",
    "webpack-merge": "^4.2.1",
    "webpack-spritesmith": "^1.0.1"
  }
}
